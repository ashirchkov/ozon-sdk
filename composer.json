{
  "name": "ashirchkov/ozon-sdk",
  "license": "MIT",
  "description": "Php SDK for OZON api services",
  "require": {
    "php": ">=8.2",
    "ext-json": "*",
    "psr/http-client": "1.0.1",
    "nyholm/psr7": "1.4.0",
    "symfony/serializer": "^7.2",
    "symfony/serializer-pack": "^1.3"
  },
  "require-dev": {
    "guzzlehttp/guzzle": "^7.0",
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "AlexeyShirchkov\\Ozon\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AlexeyShirchkov\\Ozon\\Tests\\": "tests/"
    }
  }
}
